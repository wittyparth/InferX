              },
              {
                "key": "Content-Type",
                "value": "application/json"
              }
            ],
            "body": {
              "mode": "raw",
              "raw": "{\"url\": \"https://your-service.com/webhook\", \"events\": [\"prediction.made\", \"prediction.failed\"], \"model_id\": \"{{MODEL_ID}}\", \"active\": true}"
            },
            "url": {
              "raw": "{{BASE_URL}}/webhooks/create",
